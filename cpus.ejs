<script type="text/javascript" charset="utf-8">
    function init() {
        mygrid = new dhtmlXGridObject("Mgrid_here");
        mygrid.setImagePath("../../codebase/imgs/");
        mygrid.setHeader("CPU Name, Year Released, # of Cores, Clock speed, Cache (in MB),Motherboard Socket");
        mygrid.attachHeader("","","","","","");
        mygrid.setColumnIds("CName,YearReleased,Cores,ClockSpeed,Cache,SocketType");

        mygrid.setInitWidths("80,100,100,90,100,128"); 
        mygrid.enableResizing("false,false,false,false,false,false");
       
        mygrid.setColTypes("ed,ed,ed,ed,ed,ed");
        mygrid.setColSorting("str,str,str,str,str,str");
        mygrid.setSkin("dhx_skyblue");
        mygrid.init();
		 mygrid.load("/CPUs", "js");
		 
		 var dp = new dataProcessor("/CPUs");
		 dp.init(mygrid);
		 dp.enableDataNames(true);
		 dp.setTransactionMode("POST", false);
	}
	
	function addrow(){
		var id = mygrid.uid();
		mygrid.setActive(true);
		mygrid.addRow(id, "Name,YearReleased,Cores,ClockSpeed,Cache,SocketType");
       
	}
	
	function deleterow(){
		mygrid.deleteSelectedRows();

	}

</script>   	
