        	<h1>Graphics Cards Table</h1>
            
            
            

          <!--  <input style="padding:6px;" type="button" value="Add a new CPU" onclick="addrow()">
            <input style="padding:6px;" type="button" value="Delete the selected record" onclick="deleterow()">-->
            <h2>Add a new entry</h2>
            <p>Please use the form below to enter in a new CPU:</p>


<form method="POST" action="<%= action %>">
            <fieldset>
<legend>Graphics Card Information(<span class="required">required</span>)</legend>
<div class="col1">

             <label for="Name">Name of Card:</label>
                <br />
                <input type="text" name="Name" id="Name" />
            


            <br /><br />
              <label for="YearReleased">Year of Graphics Card Release:</label>
            <select name="YearReleased" id="YearReleased">
                <option value="2010">2010</option>
                <option value="2011">2011</option>
                <option value="2012">2012</option>
                <option value="2013">2013</option>
                <option value="2014">2014</option>
            </select>
            <br /><br />

               
                <label for="Expansion">Graphics Card Expansion:</label>
                <br />
                <input type="text" name="Expansion" id="Expansion" /> 
                
           </div>
           <div class="col2">


                <label for="ClockSpeed">Clock Speed:</label>
                <br />
                <input type="text" name="ClockSpeed" id="ClockSpeed" />
            
            <br />
                <label for="VRAM">Video RAM Size (In Gigabytes):</label>
                <br />
                <input type="number" name="VRAM" id="VRAM" min = "1" max = "12"/>


               <br />
               <label for="Chipset">Chipset Manufacture:</label>
            <select name="Chipset" id="Chipset">
                <option value="Nvidia">Nvidia</option>
                <option value="AMD">AMD</option>
                
            </select>
            </div>
            </fieldset>
            <br />
            <input type="submit">
            </form>




            
            
